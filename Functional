def add_number(num, num1):
    return num + num1


def sub_number(num, num1):
    return num - num1


def name_print(name, surname):
    return "Name: " + name + ", " + "Surname: " + surname


def func_two_arg(option, val, val1):
    print(option(val, val1))


func_two_arg(add_number, 10, 10)
func_two_arg(sub_number, 10, 5)
func_two_arg(name_print, "Munir", "Kakar")


def loop():
    for x in range(10):
        func_two_arg(add_number, x, x + 5)


loop()
